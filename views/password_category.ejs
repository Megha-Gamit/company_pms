<%- include('header')%>
    
    <div class="container">
        <h1><%= title %></h1>
        <table class="table table-striped table-primary">
          <thead>
            <tr>
              <th>Password Category List</th>
             
              <th>Action</th>
            </tr>
          </thead>
        
          <% if(records.length>0){

            records.forEach(function(row){%>
         
         
          <tr>
            
            <td><%= row.Password_category %></td>
            <td><a class="btn btn-primary btn-xs" href="/passwordCategory/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
            <a class="btn btn-danger btn-xs" href="/passwordCategory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a></td>
          </tr>
    
          <%  }) } %>

        </table>
    
      </div>
</body>
</html>